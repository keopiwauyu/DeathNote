{
   "name": "keopiwauyu/deathnote",
   "type": "library",
   "require": {
      "pocketmine/pocketmine-mp": "4.0.0",
      "sylvrs/libmarshal": "^1.4",
      "sof3/await-generator": "^3.4",
      "sof3/await-std": "dev-master",
      "keopiwauyu/piggycustomenchants": "dev-master"
   },
   "require-dev": {
      "phpunit/phpunit": "^9.5",
      "phpstan/phpstan": "1.4.6",
      "phpstan/phpstan-strict-rules": "^1.0",
      "phpstan/extension-installer": "^1.0"
   },
   "autoload": {
      "psr-0": {
         "keopiwauyu\\DeathNote\\": "src"
      }
   },
   "autoload-dev": {
      "psr-4": {
         "keopiwauyu\\HyundaiCommando\\": "tests"
      }
   },
   "config": {
      "allow-plugins": {
         "phpstan/extension-installer": true
      }
   }
}